{
  "compilerOptions": {
    "target": "es6",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "paths": {
      "src/*": [
        "*",
      ],
    },
    "skipLibCheck": true,
    "baseUrl": "./src",
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
    "src/**/*.unused.*"
  ]
}
