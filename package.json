{
  "name": "utility-discord-bot",
  "version": "1.0.0",
  "engines": {
    "node": "16.x"
  },
  "description": "https://discord.com/api/oauth2/authorize?client_id=783752800138952744&permissions=1573383287&scope=bot",
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint .",
    "test:unit": "jest",
    "test": "npm run lint && jest && tsc",
    "shell:bot": "docker exec -it utility_discord_bot sh",
    "shell:db": "docker exec -it utility_discord_bot_db sh -c \"psql -d utility_discord_bot_db -U user\"",
    "dev": "nodemon",
    "start": "ts-node --transpile-only --require tsconfig-paths/register src/index.ts",
    "start:nohup": "nohup npm start > log 2>&1 &",
    "stop:nohup": "pkill -9 -f 'bin/ts-node'",
    "restart:nohup": "npm run stop:nohup\nnpm run start:nohup",
    "up": "docker-compose up",
    "docker-push": "./docker-push.sh",
    "docker-push-latest": "./docker-push.sh latest",
    "docker-pull-latest": "docker pull mikeyaworski/utility-discord-bot:latest",
    "docker-start": "docker run -d --env-file .env --name utility-discord-bot mikeyaworski/utility-discord-bot:latest",
    "docker-stop": "docker stop utility-discord-bot && docker rm utility-discord-bot",
    "docker-logs": "docker logs -f utility-discord-bot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeyaworski/utility-discord-bot.git"
  },
  "author": "Michael Yaworski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikeyaworski/utility-discord-bot/issues"
  },
  "homepage": "https://github.com/mikeyaworski/utility-discord-bot#readme",
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/opus": "^0.6.0",
    "@discordjs/rest": "0.1.0-canary.0",
    "@discordjs/voice": "^0.6.0",
    "@vvo/tzdb": "^6.7.0",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.10",
    "chess.js": "^0.12.0",
    "chrono-node": "^2.3.0",
    "cron": "^1.8.2",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0",
    "dotenv": "^8.2.0",
    "emoji-regex": "^9.2.0",
    "express": "^4.17.1",
    "humanize-duration": "^3.27.1",
    "ini": "^2.0.0",
    "lodash.chunk": "^4.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.throttle": "^4.1.1",
    "lodash.uniq": "^4.5.0",
    "node-notifier": "^9.0.0",
    "p-limit": "^3.1.0",
    "pg": "^8.5.1",
    "play-dl": "^1.8.8",
    "sequelize": "^6.17.0",
    "spotify-url-info": "^2.2.3",
    "tweetnacl": "^1.0.3",
    "youtube-dl-exec": "^2.0.2",
    "youtube-sr": "^4.1.11"
  },
  "devDependencies": {
    "@types/chess.js": "^0.11.2",
    "@types/cron": "^1.7.3",
    "@types/emoji-regex": "^9.2.0",
    "@types/express": "^4.17.9",
    "@types/humanize-duration": "^3.27.1",
    "@types/jest": "^26.0.16",
    "@types/lodash.chunk": "^4.2.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/node": "^14.14.10",
    "@types/ws": "^7.4.6",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.23.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "pre-commit": "^1.2.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.5.4"
  },
  "pre-commit": [
    "test"
  ]
}
